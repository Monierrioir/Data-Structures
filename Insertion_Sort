#include<stdio.h>
void insertion(int arr[],int size);
void print(int arr[],int size);
int main(){
    int arr[]={7,3,8,5,2,9};
    int size=sizeof(arr)/sizeof(arr[0]);
    insertion(arr,size);
    print(arr,size);



    return 0;
}
void insertion(int arr[],int size){
    int i,key,j;
    for(i=1;i<=size;i++){
        key=arr[i];
        j=i-1;

        for(;j>=0 && key < arr[j];j--)
            arr[j+1]=arr[j];
        arr[j+1]=key;
    }

}
void print(int arr[],int size){

    printf("The sorted array is: \n");
    for(int a=0;a<=size;a++){
        printf("%d\n",arr[a]);
    }

}
